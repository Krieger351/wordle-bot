{
  "name": "wordle-bot",
  "version": "1.0.0",
  "description": "Bot for solving wordle",
  "main": "index.js",
  "scripts": {
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --ignore-unknown --no-error-on-unmatched-pattern --check **/*",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "fix:prettier": "prettier --ignore-unknown --no-error-on-unmatched-pattern --write **/*",
    "build": "rollup --config",
    "start": "run-p start:*",
    "start:rollup-watch": "rollup --config --watch",
    "start:serve": "http-server --ssl --cors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Krieger351/wordle-bot.git"
  },
  "author": "",
  "license": "LICENSE",
  "bugs": {
    "url": "https://github.com/Krieger351/wordle-bot/issues"
  },
  "homepage": "https://github.com/Krieger351/wordle-bot#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "esbuild": "^0.14.21",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-custom-elements": "0.0.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-explicit-type-exports": "^0.12.1",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "http-server": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-esbuild": "^4.8.2",
    "typescript": "^4.5.5"
  }
}
